<!DOCTYPE html>

<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MicroClean.Franchisee.WebApp</title>
    <script src="~/theams/plugins/jquery/jquery.min.js"></script>
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Font Awesome -->
    <link href="~/theams/plugins/fontawesome-free/css/all.css" rel="stylesheet" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!--- data table CSS----->
    <link rel="stylesheet" href="~/theams/plugins/datatables-bs4/css/dataTables.bootstrap4.min.css">
    <link rel="stylesheet" href="~/theams/plugins/datatables-responsive/css/responsive.bootstrap4.min.css">

    <!-- Theme style -->
    <link href="~/theams/dist/css/adminlte.min.css" rel="stylesheet" />
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
    <!--custome CSS-->
    <link rel="stylesheet" href="~/theams/plugins/summernote/summernote-bs4.css">
    <link href="~/theams/micro-custom.css" rel="stylesheet" />
    <script src="~/theams/dist/js/DatimePicker/jquery.datetimepicker.js"></script>
    <link href="~/theams/dist/css/DatePicker/jquery.datetimepicker.css" rel="stylesheet" />

    <!-- jQuery -->
    @*<script src="../../plugins/jquery/jquery.min.js"></script>*@


</head>
<body>
    <div class="wrapper">
        <nav class="main-header navbar navbar-expand navbar-white navbar-light">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link mobi-menu" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                </li>
            </ul>
            <ul class="navbar-nav ml-auto">
                <li class="nav-item d-sm-inline-block">
                    <button type="button" id="btnLogoff" style="background-color: #000;color: white; border-color: #000;font-size: 19px; font-weight: 600;" class="nav-link log">Logout</button>
                </li>
            </ul>
        </nav>

        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4 sidepanel">
            <!-- Brand Logo -->
            <div class="admin-logo">
                <a href="#" class="brand-link">
                    @*<img src="./microclean.jpg" alt="Admin Logo" class="brand-image img-circle elevation-3" style="opacity: .8">*@
                    <img src="~/Images/microclean.jpg" alt="Admin Logo" class="brand-image img-circle elevation-3" style="opacity: .8">
                    <span class="brand-text font-weight-light">Micro Clean</span>
                </a>
            </div>

            <!-- Sidebar -->
            <div class="sidebar">

                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">

                        <li class="nav-item">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fas fa-tachometer-alt"></i>
                                <p>Dashboard</p>
                            </a>
                        </li>

                        <li class="nav-item">
                            <a href="/Admin/Index" class="nav-link">
                                <i class="nav-icon fa fa-user" aria-hidden="true"></i>
                                <p>Franchisee Management</p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Admin/CreateFranchisee" class="nav-link">
                                <i class="nav-icon fa fa-user-circle" aria-hidden="true"></i>
                                <p>Create Franchisee</p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Admin/EmployeeList" class="nav-link">
                                <i class="nav-icon fa fa-user" aria-hidden="true"></i>
                                <p>Employee Management</p>
                            </a>
                        </li>

                        <li class="nav-item">
                            <a href="/Admin/DocumentUpload" class="nav-link">
                                <i class="nav-icon fa fa-file-text" aria-hidden="true"></i>
                                <p>Document Details</p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fa fa-video-camera" aria-hidden="true"></i>
                                <p>
                                    Training Documents
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="/Admin/TrainingDocUpload" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Upload Document</p>
                                    </a>
                                </li>
                            </ul>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="/Admin/TrainingDocumentDetail" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Document Details</p>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fa fa-pencil" aria-hidden="true"></i>
                                <p>
                                    Online Test
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>

                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="/Admin/QuestionDetail" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Question Details</p>
                                    </a>
                                </li>
                            </ul>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="/Admin/QuestionConfigure" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Add Question</p>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fa fa-cog" aria-hidden="true"></i>
                                <p>
                                    Setting
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="/Admin/ChangePassword" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Change Password</p>
                                    </a>
                                </li>

                            </ul>
                        </li>
                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>
        <!-- Main content -->
        <section class="content container-fluid">
            <!--------------------------
              | Your Page Content Here |
            -------------------------->
            @RenderBody()
        </section>
        <!-- /.content -->
    </div>

    <footer class="main-footer">
        <div class="float-right d-none d-sm-block">
        </div>
        <strong>Copyright &copy; @DateTime.Now.Year <a href="https://www.pi-infotech.com" target="_blank">PI-Infotech</a>.</strong> All rights reserved.
    </footer>
    <!-- Control Sidebar -->
    <aside class="control-sidebar control-sidebar-dark">
        <!-- Control sidebar content goes here -->
    </aside>
    <!-- /.control-sidebar -->
    <!-- jQuery -->
    <script src="~/theams/plugins/jquery/jquery.min.js"></script>
    <script src="~/theams/plugins/bs-custom-file-input/bs-custom-file-input.min.js"></script>
    <!-- Bootstrap --> 
    <!-- AdminLTE App -->
    <script src="~/theams/dist/js/adminlte.min.js"></script>
    <script src="~/theams/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- overlayScrollbars -->
    <script src="~/theams/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js"></script>
    <!-- OPTIONAL SCRIPTS -->
    <script src="~/theams/dist/js/demo.js"></script>
    <script src="~/theams/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="~/theams/plugins/datatables-bs4/js/dataTables.bootstrap4.min.js"></script>
    <script src="~/theams/plugins/datatables-responsive/js/dataTables.responsive.min.js"></script>
    <script src="~/theams/plugins/datatables-responsive/js/responsive.bootstrap4.min.js"></script>
    <!-- jQuery Mapael -->
    <script src="~/theams/plugins/jquery-mousewheel/jquery.mousewheel.js"></script>
    <script src="~/theams/plugins/raphael/raphael.min.js"></script>
    <script src="~/theams/plugins/jquery-mapael/jquery.mapael.min.js"></script>
    <script src="~/theams/plugins/jquery-mapael/maps/usa_states.min.js"></script>
    <script src="~/theams/plugins/summernote/summernote-bs4.min.js"></script>
    <script type="text/javascript">
    $(document).ready(function () {
        $("#btnLogoff").click(function () {
            debugger;
            localStorage.removeItem('token');
            localStorage.clear();
            //window.location.href = "Account/Login";
            window.location.href = '@Url.Action("Login", "Account")'
        });
    });
    </script>
    <script>
        $(document).ready(function () {
            $('.textarea').summernote();
        });
    </script>
    <script type="text/javascript">
        $(document).ready(function () {
            bsCustomFileInput.init();
        });
    </script>

    @RenderSection("Scripts", required: false)
</body>
</html>
