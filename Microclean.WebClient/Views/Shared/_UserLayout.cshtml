<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Pi Infotech | @(ViewData["Title"].ToString() ?? "")</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/theams/plugins/fontawesome-free/css/all.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
    <!-- icheck bootstrap -->
    <link rel="stylesheet" href="~/theams/plugins/icheck-bootstrap/icheck-bootstrap.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/theams/dist/css/adminlte.min.css">
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
    <link href="~/theams/microstyle.css" rel="stylesheet">
    <script src="~/theams/plugins/jquery/jquery.min.js"></script>
    <script src="~/theams/dist/js/DatimePicker/jquery.datetimepicker.js"></script>
    <link href="~/theams/dist/css/DatePicker/jquery.datetimepicker.css" rel="stylesheet" />
</head>
<body class="hold-transition">
    <!------ Custome Header start---------->
    <section id="header-style">
        <div class="container">
            <div class="row">
                <div class="col-xs-7 col-sm-6 col-lg-3 left">
                    <div class="logo-img txtcolor">
                    <img src="~/Images/microclean.jpg" alt="Admin Logo" class="brand-image img-circle elevation-3" style="opacity: .8">
                    <span class="brand-text font-weight-light">Micro Clean</span>
                    </div>
                </div>
                <div class="col-xs-6 col-sm-6 col-lg-9 right">
                    <nav class="navbar navbar-expand-sm bg-light text-center">
                        <ul class="navbar-nav">
                            <li class="nav-item dropdown menu-position">
                                <a class="nav-link dropdown-toggle" href="#" id="navbarDropdownMenuLink" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    <i class="fas fa-bars"></i>
                                </a>
                                <div class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
                                    <a class="dropdown-item" asp-action="Index" asp-controller="FranchiseeUser">Documents</a>
                                    <a class="dropdown-item" asp-action="ChangePassword" asp-controller="FranchiseeUser">Change Password</a>
                                    <a class="dropdown-item" id="btnLogoff"> Logout</a>
                                </div>
                            </li>
                        </ul>
                    </nav>
                </div>
            </div>
        </div>
    </section>
    <section id="menu-style">
        <div class="container">
            <div class="row menu-row">
            </div>
        </div>
    </section>
    @RenderBody()
    <!---  custome footer --->
    <section id="footer-style">
        <div class="container">
            <div class="row">
                <div class="col-md-4"></div>
                <div class="col-md-4 text-center">
                    <div class="social_icon">
                        <ul>
                            <li><a target="_blank" href="#"><i class="fa fa-youtube"></i></a></li>
                            <li><a target="_blank" href="#"><i class="fa fa-facebook"></i></a></li>
                            <li><a target="_blank" href="#"><i class="fa fa-instagram"></i></a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-md-4"></div>
            </div>
            <div class="row">
                <div class="col-md-3"></div>
                <div class="col-md-6">
                    <div class="copyright">
                        © 2019 <a title="Pi Infotech" target="_blank" href="https://www.pi-infotech.com/">Pi Infotech</a>. All Rights Reserved.
                    </div>
                </div>
                <div class="col-md-3"></div>
            </div>
        </div>
    </section>
    <!-- jQuery -->
    <script src="~/theams/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="~/theams/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/theams/dist/js/adminlte.min.js"></script>
    <script type="text/javascript">
        $(function () {
            $('[data-toggle="tooltip"]').tooltip()
        })
    </script>
    @RenderSection("Scripts", required: false)
</body>
</html>
<script type="text/javascript">
    $(document).ready(function () {
        $("#btnLogoff").click(function () {
            debugger;
            localStorage.removeItem('token');
            localStorage.clear();
            window.location.href = "/Account/Login";
        });
    });
</script>