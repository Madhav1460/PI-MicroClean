     function validateEmail(email) {
        debugger;
        if (email.value != '') {
            var reg = /^[\w-\._\+%]+@@(?:[\w-]+\.)+[\w]{2,6}$/;
            if (reg.test(email.value) == false) {
                $('#error_email').html("Invalid email address");
                $('#error_email').show();
                document.getElementById("txtEmail").value = '';
                return false;
            } else {
                $('#error_email').html("");
                $('#error_email').hide();
            }
            return true;
        }
    }
